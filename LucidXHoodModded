local Premium = false
local PremiumCodes = {"",""}

if game.PlaceId == 5602055394 or 2788229376 then
	
	for i,v in pairs(PremiumCodes) do
		if _G.Premium == PremiumCodes then
			Premium = true
		end
	end
	
	if syn then
		Premium = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "LucidX | Hood Modded";
			Text = "PREMIUM | Welcome "..game.Players.LocalPlayer.Name,
			Icon = "rbxassetid://0";
			Duration = 5;
		})
	else
		game.StarterGui:SetCore("SendNotification", {
			Title = "LucidX | Hood Modded";
			Text = "NON-PREMIUM | Welcome "..game.Players.LocalPlayer.Name,
			Icon = "rbxassetid://0";
			Duration = 5;
		})
	end

else
	game.Players.LocalPlayer:Kick("Game not supported")
end

